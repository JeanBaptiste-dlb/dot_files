{
   "background": {
      "scripts": [ "lib/jquery-1.8.1.min.js", "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/chrome.png",
      "default_title": "Wanteeed"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "lib/jquery-1.8.1.min.js", "lib/jquery-ui-1.10.4.min.js", "scripts/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "fr",
   "description": "__MSG_description__",
   "homepage_url": "http://www.wanteeed.com/",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/button.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "wanteeed",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "webNavigation", "cookies", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.9.4",
   "web_accessible_resources": [ "*" ]
}
